{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)\\.mjs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    {
      "source": "/pdf.min.mjs",
      "destination": "https://server-worker.vercel.app/public/pdf.min.mjs",
      "permanent": false
    }
  ]
}
